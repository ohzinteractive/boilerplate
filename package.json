    "@parcel/transformer-image": "2.0.0-beta.2",
    "@parcel/transformer-pug": "2.0.0-beta.2",
    "@parcel/transformer-sass": "2.0.0-beta.2",
    "basisu": "^1.2.1",
    "parcel": "2.0.0-beta.2",
    "replace-in-file": "^6.2.0",
    "serve-static": "^1.14.1",
    "standardx": "^7.0.0"
  },
  "private": true,
